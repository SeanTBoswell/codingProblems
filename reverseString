//Reverse a string without using any built in functions

#include <iostream>
#include <string>

using namespace std;

void reverse(string s1);

int main()
{
    string s1;
    cout << "Enter a word/phrase: ";
    getline(cin, s1);
    
    reverse(s1);
    
    return 0;
}

void reverse(string s1)
{
    for(int i = s1.length() - 1; i >= 0; i--)
    {
        cout << s1[i];
    }
}
